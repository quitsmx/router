{
  "name": "@quitsmx/router",
  "version": "1.3.4",
  "description": "Next generation Routing for React.",
  "main": "./index.js",
  "module": "./es/index.js",
  "browser": "./umd/reach-router.min.js",
  "typings": "./index.d.ts",
  "files": [
    "es",
    "index.*",
    "umd",
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node build/build.js",
    "watch": "watch -p \"src/**/*.js\" -c \"yarn build\"",
    "test": "jest --no-cache",
    "clean": "rimraf es lib umd index.js",
    "prebuild": "yarn clean",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "format": "prettier '**/*.js' --write",
    "lint": "eslint src"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testRegex": ".+\\.test\\.js$",
    "testURL": "http://localhost"
  },
  "peerDependencies": {
    "react": "17.x",
    "react-dom": "17.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-external-helpers": "~7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/react": "~17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-eslint": "~10.1.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "core-js": "~3.15.1",
    "eslint": "^7.29.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gzip-size": "^6.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "pinst": "~2.1.6",
    "prettier": "~2.3.1",
    "pretty-bytes": "^5.6.0",
    "pretty-quick": "^3.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "render-markdown-js": "^1.3.0",
    "rimraf": "~3.0.2",
    "rollup": "~2.52.2",
    "rollup-plugin-terser": "~7.0.2",
    "typescript": "~4.3.4",
    "watch-cli": "^0.2.3"
  },
  "author": "Ryan Florence",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reach/router.git"
  },
  "keywords": [
    "react",
    "react router"
  ],
  "dependencies": {
    "invariant": "^2.2.4"
  },
  "browserslist": [
    ">= 0.2% and since 2018-01",
    "safari >= 12.1",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/reach/router/issues"
  },
  "homepage": "https://github.com/reach/router#readme"
}
